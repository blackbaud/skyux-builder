{
  "extends": "airbnb-base",
  "env": {
    "jasmine": true
  },
  "rules": {
    "arrow-body-style": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "func-names": "off",
    "global-require": "off",
    "import/no-dynamic-require": "off",
    "no-multi-assign": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "operator-linebreak": "off",
    "padded-blocks": "off",
    "prefer-arrow-callback": "off",
    "prefer-destructuring": "off"
  }
}
