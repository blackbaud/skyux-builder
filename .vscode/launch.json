{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug SKY Pages",
            "type": "node2",
            "request": "launch",
            "program": "${workspaceRoot}/../sky-pages-cli/bin/sky-pages.js",
            "stopOnEntry": false,
            "args": ["build"],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "console": "integratedTerminal",
            "sourceMaps": false,
            "outFiles": []
        },
        {
            "name": "Run jasmine tests",
            "type": "node2",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/.bin/jasmine",
            "stopOnEntry": false,
            "args": ["JASMINE_CONFIG_PATH=jasmine.json"],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "console": "integratedTerminal",
            "sourceMaps": false,
            "outFiles": []
        }
    ]
}
